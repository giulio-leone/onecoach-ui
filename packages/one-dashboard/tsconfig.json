{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@onecoach/lib-stores": [
        "../../../onecoach-backend/packages/lib-stores/src/index.ts"
      ],
      "@onecoach/lib-stores/*": [
        "../../../onecoach-backend/packages/lib-stores/src/*"
      ],
      "@onecoach/types": [
        "../../../onecoach-types/src/index.ts"
      ]
    },
    "incremental": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
