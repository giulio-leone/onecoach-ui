{
  "name": "@giulio-leone/one-dashboard",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./layout": {
      "import": {
        "types": "./dist/layout/index.d.ts",
        "default": "./dist/layout/index.js"
      },
      "require": {
        "types": "./dist/layout/index.d.cts",
        "default": "./dist/layout/index.cjs"
      }
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "@giulio-leone/lib-ai": "^0.1.0",
    "@giulio-leone/lib-api": "^0.1.0",
    "@giulio-leone/lib-core": "^0.1.0",
    "@giulio-leone/lib-design-system": "^0.1.0",
    "@giulio-leone/lib-shared": "^0.1.0",
    "@giulio-leone/lib-stores": "^0.1.0",
    "@giulio-leone/types": "^0.1.0",
    "@giulio-leone/ui": "^0.1.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "peerDependencies": {
    "react": ">=19.1.0",
    "next": ">=15.0.0",
    "lucide-react": "*",
    "next-intl": "*"
  }
}
