{
  "name": "@giulio-leone/ui-layout",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./navigation": {
      "types": "./src/navigation.ts",
      "default": "./src/navigation.ts"
    },
    "./routing": {
      "types": "./src/routing.ts",
      "default": "./src/routing.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@onecoach/ui-core": "workspace:*",
    "@giulio-leone/lib-design-system": "workspace:*",
    "@giulio-leone/translations": "workspace:*",
    "@giulio-leone/hooks": "workspace:*",
    "@giulio-leone/types": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "next-intl": "^4.7.0"
  }
}
