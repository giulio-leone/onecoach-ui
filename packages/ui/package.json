{
  "name": "@giulio-leone/ui",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./components": {
      "import": {
        "types": "./dist/components.d.ts",
        "default": "./dist/components.js"
      },
      "require": {
        "types": "./dist/components.d.cts",
        "default": "./dist/components.cjs"
      }
    },
    "./components/memory": {
      "import": {
        "types": "./dist/memory.d.ts",
        "default": "./dist/memory.js"
      },
      "require": {
        "types": "./dist/memory.d.cts",
        "default": "./dist/memory.cjs"
      }
    },
    "./visual-builder": {
      "import": {
        "types": "./dist/visual-builder.d.ts",
        "default": "./dist/visual-builder.js"
      },
      "require": {
        "types": "./dist/visual-builder.d.cts",
        "default": "./dist/visual-builder.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit"
