    "react-markdown": "^10.1.0",
    "react-native-web": "~0.21.2",
    "react-syntax-highlighter": "^16.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "peerDependencies": {
    "expo-linear-gradient": "*",
    "react": ">=19.1.0 <20",
