{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "paths": {
      "ui/*": [
        "./src/*"
      ],
      "lib/*": [
        "../lib/src/*"
      ],
      "design-system/*": [
        "../design-system/src/*"
      ],
      "@onecoach/constants": [
        "../../../onecoach-constants/src/index.ts"
      ],
      "@onecoach/constants/*": [
        "../../../onecoach-constants/src/*"
      ],
      "@onecoach/types": [
        "../../../onecoach-types/src/index.ts"
      ],
      "@onecoach/types/*": [
        "../../../onecoach-types/src/*"
      ],
      "@onecoach/schemas": [
        "../../../onecoach-schemas/src/index.ts"
      ],
      "@onecoach/schemas/*": [
        "../../../onecoach-schemas/src/*"
      ],
      "types": [
        "../types/src/index.ts"
      ],
      "types/*": [
        "../types/src/*"
      ],
      "hooks/*": [
        "../hooks/src/*"
      ],
      "app/navigation": [
        "../app/src/navigation/index.ts"
      ],
      "app/navigation/server": [
        "../app/src/navigation/server.ts"
      ]
    },
    "strict": true,
    "jsx": "react-jsx",
    "incremental": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "src/**/*.native.tsx",
    "src/**/*.native.ts"
  ]
}
