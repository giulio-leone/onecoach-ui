{
  "name": "@onecoach/hooks",
  "version": "0.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    },
    "./useForm": {
      "types": "./src/useForm.ts",
      "default": "./src/useForm.ts"
    },
    "./useFeatureFlag": {
      "types": "./src/useFeatureFlag.ts",
      "default": "./src/useFeatureFlag.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@onecoach/lib-shared": "workspace:*",
    "@onecoach/types": "workspace:*",
    "@onecoach/schemas": "workspace:*",
    "@onecoach/constants": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=19.1.0 <20",
    "react-native": ">=0.81 <0.83"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "sideEffects": false
}
