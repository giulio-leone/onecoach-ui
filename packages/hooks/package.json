{
  "name": "@giulio-leone/hooks",
  "version": "0.1.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@giulio-leone/constants": "^0.1.0",
    "@giulio-leone/lib-ai": "^0.1.0",
    "@giulio-leone/lib-api-client": "^0.1.0",
    "@giulio-leone/lib-core": "^0.1.0",
    "@giulio-leone/lib-shared": "^0.1.0",
    "@giulio-leone/lib-stores": "^0.1.0",
    "@giulio-leone/one-agent": "^0.1.0",
    "@giulio-leone/one-agent-hooks": "^0.1.0",
    "@giulio-leone/schemas": "^0.1.0",
    "@giulio-leone/translations": "^0.1.0",
    "@giulio-leone/types": "^0.1.0",
    "@giulio-leone/ui": "^0.1.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7"
  },
  "peerDependencies": {
    "react": ">=19.1.0 <20",
    "react-native": ">=0.81 <0.83"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  }
}
