{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "vitest/globals",
      "@testing-library/jest-dom"
    ],
    "paths": {
      "hooks/*": [
        "./src/*"
      ],
      "lib/*": [
        "../lib/src/*"
      ],
      "@onecoach/constants": [
        "../../../onecoach-constants/src/index.ts"
      ],
      "@onecoach/constants/*": [
        "../../../onecoach-constants/src/*"
      ],
      "@onecoach/types": [
        "../../../onecoach-types/src/index.ts"
      ],
      "@onecoach/types/*": [
        "../../../onecoach-types/src/*"
      ],
      "@giulio-leone/types": [
        "../../../onecoach-types/src/index.ts"
      ],
      "@giulio-leone/types/*": [
        "../../../onecoach-types/src/*"
      ],
      "@giulio-leone/types/core": [
        "../../../onecoach-types/src/auth.types.ts"
      ],
      "@onecoach/schemas": [
        "../../../onecoach-schemas/src/index.ts"
      ],
      "@onecoach/schemas/*": [
        "../../../onecoach-schemas/src/*"
      ],
      "@onecoach/lib-stores": [
        "../../../onecoach-backend/packages/lib-stores/src/index.ts"
      ],
      "@onecoach/lib-stores/*": [
        "../../../onecoach-backend/packages/lib-stores/src/*"
      ],
      "types": [
        "../types/src/index.ts"
      ],
      "types/*": [
        "../types/src/*"
      ],
      "@/*": [
        "../../apps/next/*"
      ],
      "ui/*": [
        "../ui/src/*"
      ]
    },
    "strict": true,
    "incremental": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "src/**/*.native.tsx",
    "src/**/*.native.ts"
  ]
}
