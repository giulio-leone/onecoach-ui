{
  "name": "@giulio-leone/ui-ai",
  "version": "0.6.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./ai-types": {
      "types": "./dist/ai-types.d.ts",
      "import": "./dist/ai-types.js",
      "default": "./src/ai-types.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@giulio-leone/constants": "workspace:*",
    "@giulio-leone/lib-ai": "workspace:*",
    "@giulio-leone/lib-api": "workspace:*",
    "@giulio-leone/lib-design-system": "workspace:*",
    "@giulio-leone/lib-shared": "workspace:*",
    "@giulio-leone/schemas": "workspace:*",
    "@giulio-leone/types": "workspace:*",
    "@giulio-leone/ui": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "private": true
}
